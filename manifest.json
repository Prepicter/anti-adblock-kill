{
    "manifest_version": 3,
    "name": "Remove Dialog and Overflow",
    "version": "1.0",
    "description": "Removes specific dialog container and disables overflow hidden",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ]
  }
  